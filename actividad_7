tupla_arboles=('lapacho', 'crespon', 'paraiso', 'quebracho', 'espinillo', 'aromo', 'cerezo', )
print(tupla_dinos)
print(tupla_calles)
print(tupla_arboles)
print(tupla_dinos[3])

